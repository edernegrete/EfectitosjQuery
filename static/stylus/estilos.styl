azulito = #74A6BD
cafecito = #AB988B
naranjita = #EB8540
body
	background-color cafecito
	margin 0
	padding 0
	font-size 16px
	header
		h1
			text-align center
	nav
		background-color naranjita
		border-radius 0 0 1em 1em
		box-shadow 0 1px 4px
		float left
		margin-top -20%
		padding 0
		transition all 1s ease
		ul
			text-align center
			margin-left 0
			padding 0 
		ul li
			display inline-block
			font-size 2em
		ul li:nth-child(even)
			margin 0 1em 0 1em